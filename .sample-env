# pylint: skip-file
# .env file

# port application is served on
PORT=8000

# A boolean that turns on/off debug mode; set to any of 'true', 'on', 'ok', 'y', 'yes', '1' to enable
# https://docs.djangoproject.com/en/4.1/ref/settings/#debug
DEBUG=on

# A boolean that turns on/off development mode; set to any of 'true', 'on', 'ok', 'y', 'yes', '1' to enable
DEVELOPMENT=true

# Secret key for a particular Django installation
# https://docs.djangoproject.com/en/4.1/ref/settings/#std-setting-SECRET_KEY
SECRET_KEY='this-is-a-very-secret-key'

# Database url
# https://docs.djangoproject.com/en/4.1/ref/settings/#databases
DATABASE_URL=postgresql://username:password@localhost/soapbox

# Cloudinary url
# https://pypi.org/project/dj3-cloudinary-storage/
CLOUDINARY_URL=cloudinary://1234567890123:abcdefghijklm@opqrstuv

# hostname of application on Heroku; e.g. soapbox-opinions.herokuapp.com
# https://docs.djangoproject.com/en/4.1/ref/settings/#allowed-hosts
#HEROKU_HOSTNAME = <app>.herokuapp.com